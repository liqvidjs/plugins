{
  "name": "@lqv/gsap",
  "version": "0.1.0",
  "description": "GSAP compatibility layer for Liqvid plugins",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "pnpm build:clean; pnpm build:js; pnpm build:postclean",
    "build:clean": "rm -rf dist",
    "build:js": "tsc --module esnext --outDir dist/esm; tsc --module commonjs --outDir dist/cjs",
    "build:postclean": "find dist -name tsconfig.tsbuildinfo -delete",
    "lint": "eslint --ext ts,tsx --fix src"
  },
  "author": "Yuri Sulyma <yuri@liqvidjs.org>",
  "license": "MIT",
  "devDependencies": {
    "@lqv/playback": "workspace:^",
    "gsap": "^3.10.4"
  },
  "peerDependencies": {
    "@lqv/playback": "workspace:^",
    "gsap": ">=3.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "gsap": {
      "optional": true
    }
  },
  "type": "module"
}